{
	"version": 2,
	"devCommand": "python3 main.py",
  	"builds": [
	    {
	    	"src": "/server/api.py",
	    	"use": "@vercel/python",
	    	"config": {
	    		"maxLambdaSize": "15mb",
	    		"runtime": "python3.12"
	    	}
	    }
  	],
  	"routes": [
	    {
	    	"src": "/(.*)",
	    	"dest": "server/api.py"
	    }
  	]
}
