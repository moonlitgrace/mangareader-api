{
	"version": 2,
	"devCommand": "python3 main.py",
	"env": {
		"POETRY_VERSION": "1.8.2",
	    "PYTHONPATH": "app/"
	},
  	"builds": [
	    {
	    	"src": "/app/main.py",
	    	"use": "@vercel/python",
	    	"config": {
	    		"maxLambdaSize": "15mb",
	    		"runtime": "python3.12"
	    	}
	    }
  	],
  	"routes": [
	    {
	    	"src": "/(.*)",
	    	"dest": "app/main.py"
	    }
  	]
}
